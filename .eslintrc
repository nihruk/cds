{
  "extends": ["eslint:recommended"],
  "parser": "@babel/eslint-parser",
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "parserOptions": {
    "requireConfigFile": false,
    "sourceType": "module",
    "allowImportExportEverywhere": true
  },
  "rules": {
    "no-sparse-arrays": 0,
    "no-unused-vars": "off",
    "no-prototype-builtins": "off",
    "no-self-assign": "off",
    "no-cond-assign": "off"
  }
}
