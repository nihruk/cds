    "sass-loader": "^10.1.1",
    "style-loader": "^2.0.0",
    "stylelint": "^14.14.0",
    "stylelint-config-standard": "^28.0.0",
    "stylelint-scss": "^4.3.0",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
